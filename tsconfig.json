{
  "compilerOptions": {
    "module": "NodeNext",
    "target": "ES2022",
    "lib": ["es7", "dom"],
    "jsx": "react-jsx",
    "moduleResolution": "NodeNext",
    "sourceMap": false,
    "esModuleInterop": true,
    "declaration": true,
    "noEmitOnError": true,
    "noUnusedLocals": true,
    "skipLibCheck": true,
    "baseUrl": "src",
    "rootDir": "src",
    "outDir": "dist"
  }
}
